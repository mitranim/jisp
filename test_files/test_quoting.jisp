[use `jisp:prelude.mjs` *]

;;
This is a sketch. TODO add more complicated cases.
;;

[func quotedNum [] [quote 123.456]]
[func quotedStrBacktickSimple [] [quote `some_text`]]
[func quotedStrBacktickFenced [] [quote ```some_text```]]
[func quotedStrDoubleSimple [] [quote "some_text"]]
[func quotedStrDoubleFenced [] [quote """some_text"""]]
[func quotedIdentUnqual [] [quote someName]]
[func quotedIdentAccess [] [quote nameOne.nameTwo]]
[func quotedIdentOper [] [quote !@#]]
[func quotedBrackets [] [quote [10 20 30]]]
[func unquote1 [val] [unquote val]]
[func unquote2 [val] [unquote [unquote val]]]
[func unquote3 [val] [unquote [unquote [unquote val]]]]
[func quoteUnquote [val] [quote [unquote val]]]
[func quoteUnquoteBrackets [val] [quote [10 [unquote val] `str`]]]
